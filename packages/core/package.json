{
  "name": "@tailgrid/core",
  "version": "0.1.0",
  "description": "Framework-agnostic core for TailGrid - sorting, filtering, pagination",
  "keywords": [
    "data-grid",
    "table",
    "sorting",
    "filtering",
    "pagination"
  ],
  "homepage": "https://tailgrid.dev",
  "bugs": "https://github.com/Tail-Labs/tailgrid/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tail-Labs/tailgrid.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": "sivatail <hello@taillabs.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@tanstack/table-core": "^8.20.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
