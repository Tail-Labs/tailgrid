{
  "name": "@tailgrid/ai",
  "version": "0.1.1",
  "description": "AI query parsing for TailGrid - natural language filtering and sorting",
  "keywords": [
    "ai",
    "natural-language",
    "query-parsing",
    "openai",
    "anthropic",
    "ollama"
  ],
  "homepage": "https://tailgrid.dev",
  "bugs": "https://github.com/Tail-Labs/tailgrid/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tail-Labs/tailgrid.git",
    "directory": "packages/ai"
  },
  "license": "MIT",
  "author": "sivatail <hello@taillabs.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@tailgrid/core": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
