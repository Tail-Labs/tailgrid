{
  "name": "@tailgrid/react",
  "version": "0.0.1",
  "description": "React components for TailGrid - the AI-native data grid",
  "keywords": ["react", "data-grid", "table", "ai", "tanstack-table", "tailwind"],
  "homepage": "https://tailgrid.dev",
  "bugs": "https://github.com/Tail-Labs/tailgrid/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tail-Labs/tailgrid.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "author": "sivatail <hello@taillabs.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@tailgrid/core": "workspace:*",
    "@tanstack/react-table": "^8.20.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
