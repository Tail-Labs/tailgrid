{
  "name": "@tailgrid/react",
  "version": "0.1.0",
  "description": "React components for TailGrid - the AI-native data grid",
  "keywords": [
    "react",
    "data-grid",
    "table",
    "ai",
    "tanstack-table",
    "tailwind",
    "virtual-scroll",
    "infinite-scroll"
  ],
  "homepage": "https://tailgrid.dev",
  "bugs": "https://github.com/Tail-Labs/tailgrid/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tail-Labs/tailgrid.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "author": "sivatail <hello@taillabs.io>",
  "sideEffects": [
    "*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./themes/default.css": "./src/themes/default.css",
    "./themes/tailwind.css": "./src/themes/tailwind.css",
    "./themes/dark.css": "./src/themes/dark.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/themes",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@tailgrid/core": "workspace:*",
    "@tanstack/react-table": "^8.20.0",
    "@tanstack/react-virtual": "^3.10.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "jsdom": "^23.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "optionalDependencies": {
    "@tailgrid/ai": "workspace:*",
    "@radix-ui/react-popover": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0"
  }
}
